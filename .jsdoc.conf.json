{
    "plugins": [],
    "recurseDepth": 10,
    "opts": {
      "recurse": true,
      "destination": "./docs/"
    },
    "source": {
      "include": ["bin/cmds"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "node_modules"
    },
    "sourceType": "module",
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc", "closure"]
    },
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
    }
  }